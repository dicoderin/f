# ======== Konfigurasi Wajib ========
# Masukkan private key wallet Anda di sini (64 karakter, tanpa awalan 0x)
PRIVATE_KEY=your_64_character_private_key_here

# ======== Konfigurasi Opsional ========
# Alamat RPC untuk terhubung ke testnet Fairyring
RPC_URL=https://rpc.testnet.fairyring.com

# Alamat wallet Anda (opsional, akan otomatis dibuat dari PRIVATE_KEY jika kosong)
WALLET_ADDRESS=

# Interval (dalam menit) untuk auto-claim faucet
AUTO_CLAIM_INTERVAL=60

# Level log: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Jumlah percobaan ulang untuk request yang gagal
MAX_RETRIES=3

# Waktu timeout untuk request (dalam detik)
REQUEST_TIMEOUT=30
